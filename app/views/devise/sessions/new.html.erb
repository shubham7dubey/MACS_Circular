</head>
<body class="bg-dark">

<div class="sufee-login d-flex align-content-center flex-wrap">
  <div class="container">
    <div class="login-content">
      <div class="login-logo">
        <a href="/">
          <img class="align-content" src="/images/nitklogo.png" style="height: 60px"  alt="">
        </a>
      </div>

      <div class="login-form">
        <!--<form>-->
          <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="form-group">
            <label>Email address</label>
            <!--<input type="email" class="form-control" placeholder="Email">-->
            <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control",placeholder: "Email"  %>
          </div>
          <div class="form-group">
            <label>Password</label>
            <!--<input type="password" class="form-control" placeholder="Password">-->
            <%= f.password_field :password, autocomplete: "password",class: "form-control ", placeholder: "Password" %>
          </div>



          <div class="checkbox">
            <label>

            </label>
            <label class="pull-right">

            </label>

          </div>
          <button type="submit" class="btn btn-info btn-flat m-b-30 m-t-30">Sign in</button>
          <p class="text-center">Forgot Password? <%= render "devise/shared/links" %> </p>

        <% end %>
      </div>
    </div>
  </div>
</div>

<script src="https://cdn.jsdelivr.net/npm/jquery@2.2.4/dist/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.4/dist/umd/popper.min.js"></script>
